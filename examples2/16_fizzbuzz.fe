fn fizzbuzz(n: uint) -> string {
  const is_fizz = n % 3 == 0
  const is_buzz = n % 5 == 0

  if !is_fizz && !is_buzz {
    return n.to_string()
  }

  const str = mut ""

  if is_fizz {
    str.append("Fizz")
  }

  if is_buzz {
    str.append("Buzz")
  }

  return str
}

for n in 1..=20 {
  log.info(fizzbuzz(n))
}

